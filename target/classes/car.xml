<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
    
    
  	<!--   Default scope="singleton" At the time of configuration beans has been created -->
  	<!--    	   scope="prototype" At the time of run beans are been created, every time fresh beans created -->
    <bean id ="car1" class="com.service.Car" scope="prototype">
        <property name="model" value="Toyota"></property>
        <property name="price" value="60000"></property>
        <property name="color" value="White"></property>
    </bean>
    
    <bean id ="car2" class="com.service.Car">
        <property name="model" value="Honda"></property>
        <property name="price" value="90000"></property>
        <property name="color" value="Red"></property>
    </bean>
    
</beans>